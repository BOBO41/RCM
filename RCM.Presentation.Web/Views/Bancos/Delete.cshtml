@model RCM.Application.ViewModels.BancoViewModel

@{
    ViewData["Title"] = $"Excluir Banco - {Model.Nome}";
}

<h5>Excluir Banco - @Model.Nome</h5>

<div class="form-container col m12 s12">
    <form asp-action="Delete">
        @Html.DisplayForModel()

        <div class="row pt-2 px-1">
            <post-button icon-name="delete">Excluir</post-button>
        </div>
    </form>
</div>

<div>
    <a asp-action="Details" asp-route-id="@Model.Id">Voltar para Detalhes</a>
</div>
