@model RCM.Application.ViewModels.BancoViewModel

@{
    ViewData["Title"] = "Adicionar novo banco";
}

<h5>Adicionar</h5>

<div class="col s12 m12 form-container">
    <p class="px-1 font-bold lead">
        Informação geral
    </p>
    <form asp-action="Create">
        @Html.EditorForModel()

        <div class="row pt-2 px-1">
            <post-button icon-name="done">Confirmar</post-button>
        </div>
    </form>
</div>

<div>
    <a asp-action="Index">Voltar para a lista</a>
</div>

