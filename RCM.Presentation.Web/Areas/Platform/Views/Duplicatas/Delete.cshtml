@model RCM.Application.ViewModels.DuplicataViewModel

@{
    ViewData["Title"] = $"Excluir Duplicata - {Model.NumeroDocumento}";
}

<h5>Excluir Duplicata - @Model.NumeroDocumento</h5>

<div class="col s12 m12 form-container">
    <form asp-action="Delete">
        @Html.DisplayForModel()

        <div class="row pt-2 px-1">
            <post-button icon-name="delete">Excluir</post-button>
        </div>
    </form>
</div>

<div>
    <a asp-action="Details" asp-route-id="@Model.Id">Voltar para Detalhes</a>
</div>